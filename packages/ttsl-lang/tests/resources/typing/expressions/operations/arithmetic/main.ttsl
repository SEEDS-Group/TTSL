    var subtractionInvarid  = »true - true«;
    # $TEST$ serialization Float
    var multiplicationInvarid  = »true * true«;
    # $TEST$ serialization Float
    var divisionInvarid  = »true / true«;

    # $TEST$ serialization Float
    var negationInvarid  = »-true«;
}

